<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.1.27</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>1.1.27.0</AssemblyVersion>
    <FileVersion>1.1.27.0</FileVersion>
    <PackageProjectUrl>https://github.com/httpjsonrpcnet/httpjsonrpcnet</PackageProjectUrl>
    <RepositoryUrl>https://github.com/httpjsonrpcnet/httpjsonrpcnet</RepositoryUrl>
    <PackageLicenseUrl>https://github.com/httpjsonrpcnet/httpjsonrpcnet/blob/master/LICENSE</PackageLicenseUrl>
    <Description>JSON-RPC implementation over HTTP.</Description>
    <PackageTags>http json rpc json-rpc</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="2.0.2" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
